{
  "name": "nextjs-serverless-on-firebase-template",
  "version": "0.0.0",
  "description": "Next.js + build target: serverless + Cloud Functions for Firebase + Firebase Hosting",
  "homepage": "https://github.com/DanielGiljam/nextjs-serverless-on-firebase-template#readme",
  "bugs": "https://github.com/DanielGiljam/nextjs-serverless-on-firebase-template/issues",
  "license": "MIT",
  "private": true,
  "author": "Daniel Giljam (https://github.com/DanielGiljam)",
  "main": "src/cloud-functions/index.js",
  "repository": "github:DanielGiljam/nextjs-serverless-on-firebase-template",
  "scripts": {
    "dev": "next dev",
    "serve": "scripts/serve.bash",
    "build": "next build",
    "deploy": "npm run deploy:functions && npm run deploy:hosting",
    "deploy:functions": "ASSET_PREFIX=https://mui-react-njs-cfr-template.web.app firebase deploy --only functions:next.index,functions:next.about,functions:next.example,functions:next.lang",
    "deploy:hosting": "firebase deploy --only hosting:mui-react-njs-cfr-template",
    "deploy:serverless": "npm run deploy:serverless:functions && npm run deploy:serverless:hosting",
    "deploy:serverless:functions": "ASSET_PREFIX=https://mrnct-serverless.web.app firebase deploy --only functions:next.serverless.index,functions:next.serverless.about",
    "deploy:serverless:hosting": "firebase deploy --only hosting:serverless"
  },
  "dependencies": {
    "firebase-admin": "^8.10.0",
    "firebase-functions": "^3.3.0"
  },
  "devDependencies": {
    "@material-ui/core": "^4.9.5",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.45",
    "@material-ui/styles": "^4.9.0",
    "color": "^3.1.2",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.19.0",
    "firebase-functions-test": "^0.1.6",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^9.3.0",
    "nextjs-global-app-state": "^2.1.0",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  },
  "engines": {
    "node": "8"
  }
}
