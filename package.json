{
  "name": "nextjs-serverless-on-firebase-template",
  "version": "0.0.0",
  "description": "Next.js + build target: serverless + Cloud Functions for Firebase + Firebase Hosting",
  "homepage": "https://github.com/DanielGiljam/nextjs-serverless-on-firebase-template#readme",
  "bugs": "https://github.com/DanielGiljam/nextjs-serverless-on-firebase-template/issues",
  "license": "MIT",
  "private": true,
  "author": "Daniel Giljam (https://github.com/DanielGiljam)",
  "main": "src/cloud-functions/index.js",
  "repository": "github:DanielGiljam/nextjs-serverless-on-firebase-template",
  "scripts": {
    "serve": "firebase serve --only functions,hosting",
    "build": "next build",
    "deploy": "npm run deploy:functions && npm run deploy:hosting",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:hosting": "firebase deploy --only hosting",
    "deploy:mui-react-njs-cfr-template": "npm run deploy:mui-react-njs-cfr-template:functions && npm run deploy:mui-react-njs-cfr-template:hosting",
    "deploy:mui-react-njs-cfr-template:functions": "firebase deploy --only functions:next-index,functions:next-about",
    "deploy:mui-react-njs-cfr-template:hosting": "firebase deploy --only hosting:mui-react-njs-cfr-template",
    "deploy:not-serverless": "npm run deploy:not-serverless:functions && npm run deploy:not-serverless:hosting",
    "deploy:not-serverless:functions": "firebase deploy --only functions:next-not-serverless",
    "deploy:not-serverless:hosting": "firebase deploy --only hosting:not-serverless"
  },
  "dependencies": {
    "@material-ui/core": "^4.8.3",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/styles": "^4.8.2",
    "firebase-admin": "^8.6.0",
    "firebase-functions": "^3.3.0",
    "isomorphic-unfetch": "^3.0.0",
    "js-cookie": "^2.2.1",
    "next": "^9.1.7",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.17.0",
    "firebase-functions-test": "^0.1.6",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.1"
  },
  "engines": {
    "node": "8"
  }
}
