{
  "cSpell.words": [
    "Giljam",
    "Svenska",
    "builtins",
    "esnext",
    "nextjs",
    "predeploy",
    "serializable",
    "unfetch"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "files.autoGuessEncoding": false,
  "files.encoding": "utf8",
  "files.eol": "\n",
}
